# This file describes the network interfaces available on your system
# and how to activate them. For more information, see interfaces(5).

# The loopback network interface
mapping eth1
	script /app/etc/zone-interface-mapping.sh
	map Dom-LAN

auto Dom-LAN
iface Dom-LAN inet static
address 192.168.123.1
netmask 255.255.255.0
#network
#broadcast
#gateway 
#dns-nameservers 
#dns-search
#hwaddress ether 00:01:02:a5:10:10

auto Jarsat-WAN
iface Jarsat-WAN inet static
address 10.12.3.249
netmask 255.255.255.0
#network
#broadcast
gateway 10.12.3.249
dns-nameservers 10.12.3.249
#dns-search
#hwaddress ether 00:01:02:a5:10:10

mapping eth0
	script /app/etc/zone-interface-mapping.sh
	map eth1-test-WAN

auto eth1-test-WAN
iface eth1-test-WAN inet dhcp
#address 
#netmask 
#network
#broadcast
#gateway 
#dns-nameservers 
#dns-search
#hwaddress ether 00:01:02:a5:10:10
